    </printer>
    <printer>
      <id>printer/KONICA_MINOLTA-magicolor_2490_MF</id>
    </printer>
    <printer>
      <id>printer/KONICA_MINOLTA-magicolor_2530_DL</id>
    </printer>
    <printer>
      <id>printer/KONICA_MINOLTA-magicolor_4690MF</id>
    </printer>
    <printer>
      <id>printer/Oki-C110</id>
    </printer>
    <printer>
